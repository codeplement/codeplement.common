{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "paths": {
      "*": [
        "../../app.mobile/node_modules/*",
        "../../app.web/node_modules/*"
      ],
      "@root/*": [
        "./*"
      ]
    },
    "typeRoots": [
      "../../app.mobile/node_modules/@types",
      "../../app.web/node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ]
  }
}